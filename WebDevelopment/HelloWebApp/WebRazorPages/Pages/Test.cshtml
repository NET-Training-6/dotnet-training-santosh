@page "/myfirstpage"


<h1>Hello from my first webpage</h1>
@for (int i = 1; i <= 20; i++)
{
    <b>@i</b>
}
    <br /><br />
<h3>Print all even numbers from 1 to 100</h3>

    @for (int i = 1; i <= 100; i++)
    {
        if (i % 2 == 0)
        {
            <b>@i</b>
        }
    }
    <br /><br />
<h3>Numbers containing 9 in it from  1 to 100</h3>

    @for(var i = 0; i <= 100; i++)
    {
        if (i.ToString().Contains("9"))
        {
            <b>@i</b>
        }
    }
    <br /><br />

