@model List<Student>
<h1>Student Page</h1>

<table class="table">
  <thead>
    <tr>
      <th scope="col">Name</th>
      <th scope="col">Address</th>
      <th scope="col">Email</th>
      <th scope="col">Date Of Birth</th>
      <th scope="col">Gender</th>

    </tr>
  </thead>
  <tbody>
        @foreach(var student in Model)
        {
            <tr>
              <td>@student.Name</td>
              <td>@student.Address</td>
              <td>@student.Email</td>
              <td>@student.Dob</td>
              <td>@student.Gender</td>
            </tr>
        }
        

  </tbody>
</table>